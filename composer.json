{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"christian-riesen/otp": "1.*",
		"christian-riesen/base32": "1.*",
		"csgt/login": "dev-master",
		"csgt/menu": "2.0.*@dev",
		"csgt/cancerbero": "dev-master",
		"csgt/crud": "dev-master",
		"csgt/components": "dev-master",
		"maxxscho/laravel-tcpdf": "0.*",
		"csgt/hermes": "dev-master",
		"guzzlehttp/guzzle": "~4.0",
		"cviebrock/eloquent-sluggable": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
